{
	"$schema": "../../node_modules/wrangler/config-schema.json",
	"name": "web-development-toolbox-mcp-remote",
	"main": "index.ts",
	"compatibility_date": "2025-05-03",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"migrations": [
		{
			"new_sqlite_classes": [
				"MyMCP"
			],
			"tag": "v1"
		}
	],
	"durable_objects": {
		"bindings": [
			{
				"class_name": "MyMCP",
				"name": "MCP_OBJECT"
			}
		]
	},
	"observability": {
		"enabled": true
	}
}